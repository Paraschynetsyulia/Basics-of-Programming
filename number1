//завдання номер 1 , варіант 19
#include<iostream>
using namespace std;
int main() {
    int n;
    int a;
    cout << "Input n=" << " ";
    cin >> n;
    while (n > 10) {
        a = n / 10;
        cout << a << " ";
        n = (n - a) / 10;
 }
    cout << n << " ";
    system("pause");
    return 0;
}
